# Production Environment Variables for AWS App Runner
# These variables will be used when NODE_ENV=production

# App Environment
VITE_APP_ENV=production

# Disable mock mode for production
VITE_MOCK_MODE=false

# API Base URL - Update this to your actual backend URL
VITE_API_BASE_URL=https://your-backend-url.us-east-1.awsapprunner.com

# App Configuration
VITE_APP_TITLE=Efficiency Tracker
VITE_DEBUG=false

# Performance settings
VITE_BUILD_SOURCEMAP=false 