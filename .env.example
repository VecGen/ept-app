# Environment variables for the Efficiency Tracker app
# Copy this file to .env and update the values as needed

# API Base URL - change this when you have a backend running
VITE_API_URL=http://localhost:8000/api

# App Environment (development, staging, production)
VITE_APP_ENV=development

# Enable mock mode (set to false when connecting to real backend)
VITE_MOCK_MODE=true 